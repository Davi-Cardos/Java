
public class Demonstração {
	public static void main(String[] args) {
		Bike bicicleta1 = new Bike();
		Bike bicicleta2 = new Bike();	
		
		bicicleta1.AumentarVelocidade(30);
		bicicleta2.AumentarVelocidade(10);
		bicicleta1.DiminuirVelocidade(15);
		bicicleta2.DiminuirVelocidade(7);
		bicicleta1.MudarMarcha(3);
		bicicleta2.MudarMarcha(2);
		bicicleta1.ImprimirEstados();
		bicicleta2.ImprimirEstados();
}
}

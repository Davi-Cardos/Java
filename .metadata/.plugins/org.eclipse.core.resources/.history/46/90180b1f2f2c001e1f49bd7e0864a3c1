
public class Elipse extends Forma{
	private double a, b;


	@Override
	protected double CalcArea() {
		
		return Math.PI * (Math.sqrt(2*((a * a) + (b*b)) - ((a - b) * (a - b))/2));
	}

	@Override
	protected double Perimetro() {
		return Math.PI*(a * b);
		
	}

	@Override
	protected void Imprimir() {
		// TODO Auto-generated method stub
		
	}
	
}
